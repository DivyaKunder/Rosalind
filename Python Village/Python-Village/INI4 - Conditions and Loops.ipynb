    "f = open(filename, 'r')\n",
    "data = f.readline().split(\" \")\n",
    "a = int(data[0])\n",
    "b = int(data[1])\n",
    "f.close()\n",
    "\n",
    "def sum_of_odd(a,b):\n",
    "    sum_ = 0\n",
    "    for i in range(a,b+1):\n",
    "        if i % 2 == 1:\n",
    "            sum_ += i\n",
    "    return sum_\n",
    "\n",
